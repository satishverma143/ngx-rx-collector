{
  "name": "ng2-rx-collector",
  "version": "1.0.0-1",
  "description": "Angular 2 garbage collector for RxJS subscriptions",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "webpack-dev-server --inline",
    "build": "rm -rf dist && tsc -p tsconfig-build.json",
    "compile": "tsc",
    "validate:ts": "tslint -t verbose 'app/**/*.ts'",
    "fix": "npm update && npm run clean && npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smnbbrv/ng2-rx-collector.git"
  },
  "bugs": {
    "url": "https://github.com/smnbbrv/ng2-rx-collector/issues"
  },
  "keywords": [
    "ng2-collector",
    "ng2-rx-collector",
    "ng2-garbage-collector",
    "ng2-garbage-collector",
    "angular2-collector",
    "angular2-rx-collector",
    "angular2-garbage-collector",
    "angular2-unsubscribe",
    "angular-collector",
    "angular-rx-collector",
    "angular-garbage-collector",
    "angular-unsubscribe"
  ],
  "peerDependencies": {
    "rxjs": "^5.0.0"
  },
  "devDependencies": {
    "rxjs": "^5.0.0",
    "@angular/core": "^2.0.0",
    "@angular/router": "^3.0.0",
    "@angular/common": "^2.0.0",
    "@angular/compiler": "^2.0.0",
    "@angular/platform-browser": "^2.0.0",
    "@angular/platform-browser-dynamic": "^2.0.0",
    "@types/core-js": "0.9.28",
    "@types/crypto-js": "^3.1.28",
    "@types/jasmine": "^2.2.30",
    "@types/node": "^4.0.30",
    "ts-loader": "^0.8.2",
    "typescript": "^2.0.0",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.6.12",
    "reflect-metadata": "^0.1.3",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "pre-commit": [
    "validate:html",
    "validate:ts",
    "compile"
  ],
  "author": "bobrovs",
  "license": "MIT"
}
